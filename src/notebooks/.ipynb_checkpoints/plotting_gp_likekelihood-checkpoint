      "INFO (theano.gof.compilelock): Refreshing lock /home/star/fb90/.theano/compiledir_Linux-4.15--generic-x86_64-with-debian-buster-sid-x86_64-3.6.6-64/lock_dir/lock\n"
     ]
    }
   ],
   "source": [
    "loglike_theano = gp_theano.log_likelihood(y)\n",
    "# loglike_theano = log_likelihood(kernel, 0., diag, x, y)\n",
    "\n",
    "# Likelihood\n",
    "f = theano.function(inputs=[log_sigma, log_rho], outputs=loglike_theano, on_unused_input=\"ignore\")\n",
    "# Gradients\n",
    "g = theano.function(inputs=[log_sigma, log_rho], outputs=theano.grad(loglike_theano, [log_sigma, log_rho]), on_unused_input=\"ignore\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "likelihood_values_theano = np.zeros((n_rho, n_sigma))\n",
    "gradient_values_sigma = np.zeros((n_rho, n_sigma))\n",
    "gradient_values_rho = np.zeros((n_rho, n_sigma))\n",
    "\n",
    "for i in range(n_rho):\n",
    "    for j in range(n_sigma):\n",
    "        likelihood_values_theano[i, j] = f(ln_sigma_grid[i,j], ln_rho_grid[i,j])\n",
    "        gradient_values_sigma[i, j] = g(ln_sigma_grid[i,j], ln_rho_grid[i,j])[0]\n",
    "        gradient_values_rho[i, j] = g(ln_sigma_grid[i,j], ln_rho_grid[i,j])[1]"
   ]
  },
