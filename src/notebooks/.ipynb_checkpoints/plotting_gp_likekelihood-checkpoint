    "# SHO kernel"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize celerite_theano kernel\n",
    "S0 = tt.dscalar('S0')\n",
    "Q = tt.dscalar('Q')\n",
    "omega0 = tt.dscalar('omega0')\n",
    "\n",
    "kernel = theano_terms.SHOTerm(s0=s0, Q=Q, omega0=omega0)"
   ]
