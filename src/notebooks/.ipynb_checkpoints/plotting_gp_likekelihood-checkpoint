    "## Theano log-likelihood\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "diag = tt._shared(sigF**2)\n",
    "x = tt._shared(t)\n",
    "y = tt._shared(F)\n",
    "\n",
    "# Initialize celerite_theano kernel\n",
    "log_sigma = tt.dscalar('log_sigma')\n",
    "log_rho = tt.dscalar('log_rho')\n",
    "# log_S0 = tt.dscalar('log_S0')\n",
    "# log_w0 = tt.dscalar('log_w0')\n",
    "# log_Q = tt.dscalar('log_Q')\n",
    "\n",
    "# Set up the kernel an GP\n",
    "# kernel = terms_xo.SHOTerm(log_S0=log_S0, log_w0=log_w0, log_Q=log_Q)\n",
    "kernel = terms_xo.Matern32Term(log_sigma=log_sigma, log_rho=log_rho)\n",
    "gp_theano = GP(kernel, x, diag)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
